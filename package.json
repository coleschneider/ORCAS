    "@types/react-router-dom": "^4.3.1",
    "@types/react-toastify": "^4.0.2",
    "@types/recompose": "^0.30.5",
    "@types/sinon": "^7.0.11",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "coveralls": "^3.0.3",
    "enzyme-to-json": "^3.3.5",
    "formik": "^1.5.1",
    "node-sass": "^7.0.0",
    "prettier": "^1.16.4",
    "react-helmet": "^5.2.1",
    "react-idle": "^3.0.0",
    "react-modal": "^3.8.1",
    "react-pose": "^4.0.8",
    "react-reveal": "^1.2.2",
