  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js --output-filename out.js --entry ./cypress/integration/spec.ts",
    "swcache": "aws s3 cp s3://orcaseniors.org/service-worker.js s3://orcaseniors.org/service-worker.js --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate --content-type application/javascript --acl public-read",
    "postswcache": "aws s3 cp s3://orcaseniors.org/index.html s3://orcaseniors.org/index.html --metadata-directive REPLACE --cache-control max-age=0,no-cache,no-store,must-revalidate --content-type text/html --acl public-read",
    "test": "npm run clear-cache && npm run prettier:test && npm run lint && npm run coverage",
    "unit-test:watch": "NODE_ENV=test jest --watch --config jest-config.json",
    "predeploy": "npm run build",
    "cypress:run": "NODE_ENV=test node_modules/.bin/cypress run",
    "cypress:open": "NODE_ENV=test node_modules/.bin/cypress open",
    "clear-cache": "rimraf ./jestCache",
    "deploy": "aws s3 sync build s3://orcaseniors.org --delete --cache-control max-age=31536000,public",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E1TD5WUARF7SKP --paths '/*' && rm -rf build && npm run coveralls && npm run swcache",
    "unit-test": "NODE_ENV=test jest --config jest-config.json",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "jest --coverage --config jest-config.json",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier:test": "prettier --parser=typescript --single-quote --trailing-comma=all --print-width=120 --list-different '{src,test}/**/*.{ts,tsx}'",
    "prettier": "prettier --parser=typescript --single-quote --trailing-comma=all --print-width=120 --write '{src,test}/**/*.{ts,tsx}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/prettier": "^1.16.1",
    "@types/react-helmet": "^5.0.14",
    "@types/react-modal": "^3.8.1",
